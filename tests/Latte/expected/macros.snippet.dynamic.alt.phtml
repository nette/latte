			?>		<div<?php echo ' id="' . $this->global->snippetDriver->getHtmlId("inner-$id") . '"' ?>>
<?php
			$this->global->snippetDriver->enter("inner-$id", "dynamic");
			?>				#<?php echo LR\Filters::escapeHtml($id) /* line 4 */ ?>

<?php
			$this->global->snippetDriver->leave();
?>		</div>
<?php
			$iterations++;
		}
		$this->global->snippetDriver->leave();

	}


